{
    "name": "peibin/laravel-config-center",
    "description": "Peibin config center abstraction component for Laravel.",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "config-center"
    ],
    "authors": [
        {
            "name": "Peibin Zhu",
            "email": "zhupeibin1207@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "peibin/laravel-process": "^1.0",
        "peibin/laravel-swoole-event": "^1.0",
        "peibin/laravel-utils": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "PeibinLaravel\\ConfigCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeibinLaravelTest\\ConfigCenter\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeibinLaravel\\ConfigCenter\\ConfigCenterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
